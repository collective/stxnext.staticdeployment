<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 
 <object name="site_actions" meta_type="CMF Action Category">
 	
  <object name="staticdeployment" meta_type="CMF Action" i18n:domain="stxnext.staticdeployment">
   <property name="title" i18n:translate="static_deployment_action_title">Static deployment</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${portal_url}/@@staticdeployment-controlpanel</property>
